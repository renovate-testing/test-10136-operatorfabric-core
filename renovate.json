{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":switchToGradleLite"
  ],
  "branchPrefix": "OC-1590_",
  "ignorePaths": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**"
  ],
  "npm": {
    "enabled": false
  },
  "gradle-lite": {
    "fileMatch": ['(^|/)versions.properties$', '(^|/)gradle.properties$', '\\.gradle(\\.kts)?$']
  },
  "docker-compose": { "enabled": false },
  "dockerfile": { "enabled": false },
  "gradle-wrapper": { "enabled": false },
  "sbt": { "enabled": false }
}
